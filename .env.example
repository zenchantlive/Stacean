# Stacean Gateway Poller Configuration
# Copy this to .env and fill in your values

# Stacean App URL (Vercel deployment)
STACEAN_API_URL=https://stacean.vercel.app

# Secret for Stacean API authentication (must match .env.local in stacean-repo)
STACEAN_SECRET=your-stacean-secret-here

# OpenClaw Gateway URL
OPENCLAW_URL=http://localhost:18789

# OpenClaw Gateway Auth Token (optional, for remote Gateway)
OPENCLAW_TOKEN=

# Poll interval in milliseconds
POLL_INTERVAL_MS=2000